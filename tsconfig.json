{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "esnext",
    "jsx": "react",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules", "**/node_modules/*"]
}
