mixin links
    nav.links
        a(href="/index.html#event") Events
        a(href="/index.html#about") About Us
        a(href="/index.html#contact") Contact
style
    include topbar.css
.hamburgerexpanded
    input#menuopen(type="checkbox" hidden)
    label.hamburger(for="menuopen")
        i.fas.fa-bars.open
        i.fas.fa-times.close
    label.clickaway(for="menuopen")
    +links
header
    a#logolink(href="/")
        img(src="Logo/SVG/White Brain.svg")
        h2 UTMIST
    +links